<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lab.zhang.hermes.dao.IndicatorOperatorDao">

    <resultMap id="BaseResultMap" type="lab.zhang.hermes.entity.operator.IndicatorOperatorEntity" >
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="clazz" property="clazz" jdbcType="VARCHAR" />
    </resultMap>

    <sql id="BaseColumnList" >
        id, clazz
    </sql>

    <select id="findAll" resultMap="BaseResultMap"  >
        SELECT
            <include refid="BaseColumnList" />
        FROM indicator_operator
    </select>

    <select id="findOne" parameterType="java.lang.Long" resultMap="BaseResultMap" >
        SELECT
            <include refid="BaseColumnList" />
        FROM indicator_operator
        WHERE id = #{id}
    </select>

    <insert id="insert" parameterType="lab.zhang.hermes.entity.operator.IndicatorOperatorEntity" useGeneratedKeys="true" >
        INSERT INTO
            indicator_operator (clazz)
        VALUES
            (#{clazz})
    </insert>

    <update id="update" parameterType="lab.zhang.hermes.entity.operator.IndicatorOperatorEntity" >
        UPDATE
            indicator_operator
        <trim prefix="SET" suffixOverrides=",">
            <if test="clazz != null">clazz = #{clazz},</if>
        </trim>
        WHERE id = #{id}
    </update>

    <delete id="delete" parameterType="java.lang.Long" >
        DELETE FROM
            indicator_operator
        WHERE
            id =#{id}
    </delete>

</mapper>
